{
  "name": "@ogea12/eslint-config",
  "description": "Config' ESLint appliquÃ©e dans les projets de l'OGEA 12",
  "version": "1.1.1",
  "engines": {
    "node": ">=20.17.0"
  },
  "type": "module",
  "files": [
    "build/src",
    "build/index.js",
    "build/index.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./build/index.js",
      "types": "./build/index.d.ts"
    },
    "./configs/*": {
      "import": "./build/src/configs/*.js",
      "types": "./build/src/configs/*.d.ts"
    }
  },
  "scripts": {
    "clean": "del-cli build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "compile": "npm run clean && tsc",
    "prebuild": "npm run format && npm run lint:fix",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "release": "npx release-it"
  },
  "homepage": "https://github.com/ogea12/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ogea12/eslint-config.git"
  },
  "devDependencies": {
    "@ogea12/prettier-config": "^1.0.3",
    "@ogea12/tsconfig": "^1.0.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/node": "^22.13.10",
    "del-cli": "^6.0.0",
    "prettier": "^3.5.3",
    "release-it": "^18.1.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@adonisjs/eslint-plugin": "^2.0.0-beta.5",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-n": "^17.16.2",
    "eslint-plugin-perfectionist": "^4.10.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-yml": "^1.17.0",
    "local-pkg": "^1.1.1",
    "typescript-eslint": "^8.26.0"
  },
  "peerDependencies": {
    "eslint": "^9.20.0"
  },
  "author": "OGEA 12",
  "license": "MIT",
  "keywords": [
    "ogea12",
    "eslint"
  ],
  "prettier": "@ogea12/prettier-config",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagAnnotation": "v${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}",
      "web": true
    }
  }
}
